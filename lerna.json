{
  "packages": [
    "packages/*"
  ],
  "version": "1.3.2",
  "npmClient": "yarn",
  "useWorkspaces": true,
  "command": {
    "bootstrap": {
      "npmClientArgs": [
        "--ignore-engines"
      ]
    },
    "version": {
      "message": "solution: publish %s",
      "allowBranch": ["master", "release/*"]
    }
  }
}
